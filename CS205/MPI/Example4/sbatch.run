#!/bin/bash
#SBATCH -J poisson_mpi
#SBATCH -o poisson_mpi.out
#SBATCH -e poisson_mpi.out
#SBATCH -p seas_iacs
#SBATCH -n 4
#SBATCH -t 0-00:30
#SBATCH --mem-per-cpu=1000

# Set up environment
module load intel/15.0.0-fasrc01
module load openmpi/1.10.2-fasrc01

# Run program
srun -n $SLURM_NTASKS --mpi=pmi2 ./poisson_mpi 128
